{
  "name": "space",
  "version": "1.0.1",
  "scripts": {
    "build": "npm run compile && npm run copy",
    "compile": "webpack --config config/webpack.config.js",
    "copy": "npm run copy:public",
    "copy:public": "copyfiles -u 1 public/**/**/*.* public/**/*.* public/*.* dist",
    "dev": "webpack serve --config config/webpack.config.dev.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:js": "eslint \"./src/**/*.js\""
  },
  "dependencies": {
    "@babel/core": "7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.1.3",
    "babel-loader": "8.2.3",
    "copyfiles": "^2.0.0",
    "core-js": "^3.25.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "matter-js": "^0.19.0",
    "mini-css-extract-plugin": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "sass": "^1.54.6",
    "sass-loader": "^13.0.2",
    "socket.io-client": "^4.7.5",
    "stylelint": "^13.6.1",
    "stylelint-order": "^4.1.0",
    "uuid": "^9.0.1",
    "three": "^0.150.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "webpack-dev-server": "^4.11.1"
  },
  "stylelint": {
    "extends": "./config/.stylelintrc.yml"
  },
  "eslintConfig": {
    "extends": "./config/.eslintrc.yml"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}